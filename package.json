{
  "name": "@briandlee/eevee",
  "version": "0.1.0",
  "description": "A simple environment variable extraction and validation library.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "private": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "check": "npm run check:lint && npm run check:types",
    "check:lint": "oxlint -c .oxlintrc.json",
    "check:types": "tsc --noEmit",
    "lint:fix": "oxlint -c .oxlintrc.json --fix",
    "test": "jest",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/brian-dlee/eevee.git"
  },
  "keywords": [
    "environment variables",
    "validation",
    "vanilla"
  ],
  "author": "Brian Lee <briandl92391@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brian-dlee/eevee/issues"
  },
  "homepage": "https://github.com/brian-dlee/eevee#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "oxlint": "^0.6.1",
    "prettier": "^3.3.3",
    "rollup": "^4.18.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "lint-staged": {
    "*.ts": [
      "oxlint",
      "prettier"
    ],
    "*.(md|js|json)": [
      "prettier"
    ]
  }
}
